------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "new.project/creationbutton.js" | double-click
with [get-editor "creationbutton.js" | get-text-viewer] {
    hover-text 12 4
    set-caret-pos 12 5
    key-type End
}
get-label "12 : 47" | get-property caption | equals "12 : 47" | verify-true
with [get-editor "creationbutton.js" | get-text-viewer] {
    hover-text 12 19
    set-caret-pos 13 32
    key-type Home
}
get-label "13 : 1" | get-property caption | equals "13 : 1" | verify-true
