------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "javascript.project/testfolder/firstfile.js" | double-click
with [get-editor "firstfile.js" | get-text-viewer] {
    set-caret-pos 4 2
    key-type "M1+a"
    key-type "M1+c"
}
get-view "Script Explorer" | get-tree | select "javascript.project/testfolder" | get-menu "New/New JavaScript File" 
    | click
with [get-window "Create Javascript file"] {
    get-editbox -after [get-label "Name:"] | set-text test
    get-button Finish | click
}
with [get-editor "test.js" | get-text-viewer] {
    key-type "M1+v"
    key-type "M1+s"
}
get-editor "test.js" | get-editbox | get-property text | equals "function click(e) {\n"
    + "\t\n"
    + "\t\n"
    + "}" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
