------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "javascript.project/testfolder/firstfile.js" | double-click
with [get-editor "firstfile.js" | get-editbox] {
    get-menu "Source/Generate Element Comment" | click
    key-type "M1+s"
}
get-editor "firstfile.js" | get-editbox | get-property text | equals "/**\n"
    + " * @param {Object} e\n"
    + " * @return {Object}\n"
    + " */\n"
    + "function click(e) {\n"
    + "\t\n"
    + "\t\n"
    + "}" | verify-true
get-view "Script Explorer" | get-tree | select "javascript.project/testfolder/testfile.js" | double-click
with [get-editor "testfile.js" | get-text-viewer] {
    set-caret-pos 1 2
    key-type "M2+M3+j"
}
get-button "Save (M1+S)" | click
get-editor "testfile.js" | get-editbox | get-property text | contains "/**\n"
    + " * @return {Object}\n"
    + " */\n"
    + "function closewindow() {\n"
    + "\tif (document.all) {\n"
    + "\t\tdocument.all.orderwindow.style.visibility=\"hidden\"\n"
    + "\t}\n"
    + "\tif (document.layers) {\n"
    + "\t\tdocument.orderwindow.visibility=\"hidden\"\n"
    + "\t}\n}\n\n" | verify-true
