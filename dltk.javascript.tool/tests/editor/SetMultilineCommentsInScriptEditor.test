------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "javascript.project/testfolder/firstfile.js" | double-click
with [get-editor "firstfile.js" | get-text-viewer] {
    set-caret-pos 2 1
    key-type Enter
    type-text "/*"
    key-type Enter
    key-type "M1+s"
}
get-editor "firstfile.js" | get-editbox | get-property text | equals "function click(e) {\n\n"
    + "\t/*\n"
    + "\t * \n"
    + "\t */\n"
    + "\t\n"
    + "}" | verify-true
with [get-editor "firstfile.js" | get-text-viewer] {
    set-caret-pos 4 8
    type-text " test line"
    key-type Enter
    key-type "M1+s"
}
get-editor "firstfile.js" | get-editbox | get-property text | equals "function click(e) {\n\n"
    + "\t/*\n"
    + "\t *  test line\n"
    + "\t *  \n"
    + "\t */\n"
    + "\t\n"
    + "}" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
