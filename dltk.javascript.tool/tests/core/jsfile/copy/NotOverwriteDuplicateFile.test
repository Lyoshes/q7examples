with [get-view "Script Explorer" | get-tree] {
    select "javascript.project/testfolder/firstfile.js" | get-menu Copy | click
    select "project.withduplicate/testfolder" | get-menu Paste | click
}
get-window "Confirm Overwriting" | get-button No | click
get-view "Script Explorer" | get-tree | select "project.withduplicate/testfolder/firstfile.js" | double-click
get-editor "firstfile.js" | get-editbox | get-property text | equals "function click() {\n"
    + "\tvar a = 5;\n"
    + "\tvar b = 6;\n"
    + "\tvar c = a + b;\n"
    + "\treturn c;\n"
    + "\t\n"
    + "}" | verify-true
get-editor "firstfile.js" | close
with [get-view "Script Explorer"] {
    click
    get-tree | select "javascript.project/testfolder/firstfile.js" | double-click
}
get-editor "firstfile.js" | get-editbox | get-property text | equals "function click(e) {\n"
    + "\t\n"
    + "\t\n"
    + "}" | verify-true
