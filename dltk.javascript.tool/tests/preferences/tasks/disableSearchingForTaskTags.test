------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa
Entry-Name: .description
Content-Type: text/plain

1. Add tasks in the code
2. Open "Preferences->Javascript->Task Tags"
3. Uncheck "Enable searching for Task Tags"
4. Save all changes
5. Make sure that tasks are not displayed in the Tasks view
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "javascript.project/testfolder/testfile.js" | double-click -nowait
with [get-editor "testfile.js" | get-editbox] {
    set-text-offset 0 8
    type-text "// TODO"
    key-type CR
    set-text-offset 0 14
    type-text "// FIXME"
    key-type CR
    key-type "M1+s"
}
get-preferences-menu | click
with [get-window Preferences] {
    get-tree | select "JavaScript/Task Tags"
    get-button "Enable searching for Task Tags" | click
    get-button Apply | click
    get-window "Task Tags Settings Changed" | get-button Yes | click
    get-button OK | click
}
get-view Tasks | click
get-view Tasks | get-tree | get-property itemCount | equals 0 | verify-true
