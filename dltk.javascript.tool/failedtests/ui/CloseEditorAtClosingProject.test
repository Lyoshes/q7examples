------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

with [get-view "Script Explorer" | get-tree] {
    select "javascript.project/testfolder/testfile.js" | double-click -nowait
    select "javascript.project/testfolder/firstfile.js" | double-click -nowait
}
get-menu "File/Restart" | click
wait-until-eclipse-is-ready
get-view "Script Explorer" | get-tree | select "javascript.project" | get-menu "Close Project" | click
get-view "Script Explorer" | get-tree | get-property itemCount | equals 3 | verify-true
get-tab-folder | get-property tabCount | equals 0 | verify-true
