------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-preferences-menu | click
with [get-window Preferences] {
    get-tree | select "JavaScript/Task Tags"
    get-button "Enable searching for Task Tags" | click
}
with [get-window Preferences] {
    get-button "Case sensitive task tag names" | get-property enablement | equals false | verify-true
    get-button "New..." | get-property enablement | equals false | verify-true
    get-button "Edit..." | get-property enablement | equals false | verify-true
    get-button Remove | get-property enablement | equals false | verify-true
}
with [get-window Preferences] {
    get-button Apply | click
    get-window "Task Tags Settings Changed" | get-button Yes | click
    get-button OK | click
}
get-preferences-menu | click
get-window Preferences | get-tree | select "JavaScript/Task Tags"
with [get-window Preferences] {
    with [get-button "Enable searching for Task Tags"] {
        get-property enablement | equals true | verify-true
        get-property caption | equals "Enable searching for Task Tags" | verify-true
        get-property selected | equals false | verify-true
        get-property grayed | equals false | verify-true
    }
    with [get-button "Case sensitive task tag names"] {
        get-property enablement | equals false | verify-true
        get-property caption | equals "&Case sensitive task tag names" | verify-true
    }
    get-button "New..." | get-property enablement | equals false | verify-true
    get-button "Edit..." | get-property enablement | equals false | verify-true
    get-button Remove | get-property enablement | equals false | verify-true
}
get-window Preferences | get-button Cancel | click
