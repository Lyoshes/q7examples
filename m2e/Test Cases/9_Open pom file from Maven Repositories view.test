------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-menu "Window/Show View/Other..." | click
get-window "Show View" | get-tree | select "Maven/Maven Repositories" | double-click -nowait
get-view "Maven Repositories" | get-tree 
    | select "Local Repositories/Workspace Projects/org/xored/examples/maven-module - jar/maven-module : 1.0-SNAPSHOT" 
    | double-click -nowait
get-editor "maven-module/pom.xml" | get-section Artifact | get-editbox -after [get-label "Group Id:"] 
    | get-property text | equals "org.xored.examples" | verify-true

