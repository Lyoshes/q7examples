------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Script Explorer" | get-tree | select "lua.project/src/main.lua" | double-click -nowait
with [get-editor main | get-editbox] {
    set-text-offset 0 1
    key-type TAB
    type-text for
    key-type "Ctrl+SPACE"
}
with [get-window -from "CompletionProposalPopup.createProposalSelector()"] {
    with [get-table] {
        get-property itemCount | equals 7 | verify-true
        get-item "for - For On List" | get-property caption | equals "for - For On List" | verify-true
        get-item "for - For On List" | get-property index | equals 0 | verify-true
    }
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "ford - For On Dots" | click 
    -default
get-eclipse-window | key-type "M1+s"
get-editor main | get-editbox | get-property text | equals "local function main()\n"
    + "\tfor index=1,select(\"#\", ...) do\n"
    + "\t\tlocal temp = select(index, ...)\n"
    + "\t\t\n"
    + "\tend\n"
    + "end\n"
    + "main()" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
